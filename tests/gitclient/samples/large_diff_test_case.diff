+  return <div className="wallet-wrapper">
+    <span>{((account?.lamports || 0) / LAMPORTS_PER_SOL ).toFixed(2)} SOL</span>
+    <div className="wallet-key">
+      {shortenAddress(`${wallet.publicKey}`)}
+      <Identicon address={wallet.publicKey.toBase58()} />
